Facility Int_Max_Example_Facility;

    Operation Max(restores I: Integer; restores J: Integer) : Integer;
        ensures (Max = I or Max = J) and (Max >= I and Max >= J);
    Procedure
        Max := I + J;
        If (I > J) then
            Max := -J + Max;
        end;
        If (J > I) then
            Max := -I + Max;
        end;
    end Max;

end Int_Max_Example_Facility;